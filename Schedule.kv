<CustomButton@Button>:
    font_size: 32
    color: 0,0,0,1
    size: 300,400
    background_normal: ''
    background_color: 1, 1, 1, 1
    size_hint: 0.7, 0.8


<FloatLayout>:
    CustomButton:
        text: "Nästa vecka"
        pos_hint: {"right": 1, "top": 1}


    CustomButton:
        text: "Föregående vecka"
        pos_hint: {"left": 0, "top": 1}

<ScreenOne>:
    BoxLayout:
        Label:
        text: "Vecka 1"
    BoxLayout:
        Button:
            text: "hejhej"
            on_press:
                root.manager.transition.direction = "left"
